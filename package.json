{
  "name": "promare-gradients",
  "version": "0.5.0",
  "description": "Neon background gradients",
  "author": "Ouxsoft <contact@ouxsoft.com",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "css": "npm-run-all css-compile css-minify",
    "css-compile": "node-sass --output dist/css src/scss/promare-gradients.scss --output-style nested",
    "css-minify": "cleancss -o dist/css/promare-gradients.min.css dist/css/promare-gradients.css"
  },
  "dependencies": {
    "clean-css-cli": "^5.5.0",
    "express": "^4.16.1",
    "sass": "^1.47.0"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  }
}
